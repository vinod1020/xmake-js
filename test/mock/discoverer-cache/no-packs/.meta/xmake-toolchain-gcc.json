{
  "version": "0.2.0",
  "toolchains": {
    "xgcc": {
      "commandPrefix": "",
      "commandSuffix": "",
      "descriptionPrefix": "GNU",
      "objectExtension": "o",
      "makeObjectsVariable": "OBJS",
  
      "tools": {
        "c": {
          "commandName": "gcc",
          "options": "-c",
          "deps": "-MMD -MP -MF\"$(@:%.o=%.d)\" -MT\"$(@)\"",
          "outputFlag": "-o",
          "output": "\"$@\"",
          "inputs": "\"$<\"",
          "fileExtensions": {
            "c": {
              "prefix": "C"
            }
          }
        },
        "cpp": {
          "commandName": "g++",
          "options": "-c",
          "deps": "-MMD -MP -MF\"$(@:%.o=%.d)\" -MT\"$(@)\"",
          "outputFlag": "-o",
          "output": "\"$@\"",
          "inputs": "\"$<\"",
          "fileExtensions": {
            "cpp": {
              "prefix": "CPP"
            },
            "C": {
              "prefix": "C_UPPER"
            },
            "c++": {
              "prefix": "C++"
            },
            "cxx": {
              "prefix": "CXX"
            },
            "cc": {
              "prefix": "CC"
            }
          }
        },
        "as": {
          "commandName": "gcc",
          "options": "-x assembler-with-cpp -c",
          "deps": "-MMD -MP -MF\"$(@:%.o=%.d)\" -MT\"$(@)\"",
          "outputFlag": "-o",
          "output": "\"$@\"",
          "inputs": "\"$<\"",
          "fileExtensions": {
            "S": {
              "prefix": "S_UPPER"
            },
            "asm": {
              "prefix": "ASM"
            }
          }
        },
        "cLinker": {
          "commandName": "gcc",
          "outputFlag": "-o",
          "output": "\"$@\""
        },
        "cppLinker": {
          "commandName": "g++",
          "outputFlag": "-o",
          "output": "\"$@\""
        }
      }
    }
  }
}