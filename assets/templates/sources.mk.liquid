# Collect the source files, deps and objects contributed by this sub-folder,
# using the global build variables.

{% for fileNode in fileNodes -%}
{{ fileNode.fileType.prefix }}_SRCS += {{ pathToCwd }}/{{ folder }}/{{ fileNode.shortName }}.{{ fileNode.fileType.ext }}
{{ fileNode.fileType.prefix }}_DEPS += {{ folder }}/{{ fileNode.shortName }}.d
OBJS += {{ folder }}/{{ fileNode.shortName }}.o

{% endfor -%}
