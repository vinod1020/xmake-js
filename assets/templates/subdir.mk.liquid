{% if files.c.length > 0 -%}
# Collect the C files, deps and objects contributed by this sub-folder,
# using the global build variables.

{% for file in files.c -%}
C_SRCS += {{ pathToCwd }}/{{ folder }}/{{ file }}.c
C_DEPS += {{ folder }}/{{ file }}.d
OBJS += {{ folder }}/{{ file }}.o
{% endfor -%}

# Define rules for building C files contributed by this sub-folder.
{{ folder }}/%.o: {{ pathToCwd }}/{{ folder }}/%.c
{% if isVerbose -%}
{{ tab }}@echo ' '
{{ tab }}@echo 'Building file: $(@:%.o=%.c)'
{{ tab }}@echo 'Invoking: GCC C Compiler'
{% else -%}
{{ tab }}@echo '[$(CC)]: $(@:%.o=%.c)'
{% endif -%}
{{ tab }}{% if isVerbose == false %}@{% endif %}$(CC) -c {{ options.c }} {{ includeOptions }} -MMD -MP -MF"$(@:%.o=%.d)" -MT"$(@)" -o "$@" "$<"
{% if isVerbose -%}
{{ tab }}@echo 'Finished building: $(@:%.o=%.c)'
{% endif -%}
{% endif -%}

